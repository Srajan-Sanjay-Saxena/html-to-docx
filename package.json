  "scripts": {
    "test": "jest",
    "test:all": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest test/utils",
    "test:pdf": "jest test/utils/pdf-creator.test.ts",
    "test:html-to-docx": "jest test/utils/html-to-docx.test.ts",
    "test:runner": "node test/test-runner.js",
    "build:ts": "tsc",
    "build:clean": "rimraf dist",
    "prerelease": "npm run build:prod",
    "release": "standard-version",
    "lint": "eslint --fix .",
    "lint:ts": "eslint --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prettier:check": "prettier --check '**/*.{js,ts}'",
    "prettier:fix": "prettier --write '**/*.{js,ts}'",
    "validate": "run-s lint prettier:check test",
    "build": "npm run build:clean && npm run build:ts && rollup -c",
    "build:dev": "NODE_ENV=development npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "prepare": "husky install",
    "test:ts-example": "npm run build && cross-env TS_NODE_PROJECT=example/typescript/tsconfig.json ts-node -r tsconfig-paths/register example/typescript/typescript-example.ts"
  },
